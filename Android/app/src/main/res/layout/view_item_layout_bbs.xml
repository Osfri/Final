<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        tools:context=".MainActivity">



    <TextView
            android:id="@+id/bbsRecycleViewTitle"
            android:text="타이틀"
            android:layout_width="119dp"
            android:layout_height="38dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"/>
    <TextView
            android:id="@+id/bbsRecycleViewId"
            android:text="아이디"
            android:layout_width="148dp"
            android:layout_height="38dp"
            app:layout_constraintTop_toTopOf="@+id/bbsRecycleViewTitle"
            app:layout_constraintStart_toEndOf="@+id/bbsRecycleViewTitle"
            android:layout_marginStart="16dp"/>
    <TextView
            android:id="@+id/bbsRecycleViewCount"
            android:text="조회수"
            android:layout_width="255dp"
            android:layout_height="38dp"
            app:layout_constraintTop_toTopOf="@+id/bbsRecycleViewId"
            app:layout_constraintStart_toEndOf="@+id/bbsRecycleViewId"
            android:layout_marginStart="12dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>