<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".signinAf.SemiIndividualActivity">

    <EditText
            android:layout_width="307dp"
            android:layout_height="58dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/semiInvi_Edit_HospitalCode"
            android:hint="병원코드를 입력하세요"
            android:gravity="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.416"
            app:layout_constraintHorizontal_bias="0.501"/>
    <Button
            android:id="@+id/semiInvi_Btn_HospitalCode"
            android:text="코드 등록"
            android:layout_width="146dp"
            android:layout_height="80dp"
            android:background="@drawable/button_round"
            android:textSize="20dp"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="360dp"/>
    <TextView
            android:id="@+id/semiInvi_Text_HospitalCode"
            android:text="(텍뷰 위치확인용) 코드번호가 일치하지 않습니다"
            android:layout_width="307dp"
            android:layout_height="wrap_content"
            android:textColor="@color/pointred"
            android:gravity="center"
            android:textSize="10dp"
            app:layout_constraintTop_toBottomOf="@+id/semiInvi_Edit_HospitalCode"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>