<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".signinAf.SemiManagementActivity">

    <EditText
            android:id="@+id/semiMana_Edit_HospitalAddress"
            android:layout_width="307dp"
            android:layout_height="58dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:hint="병원주소를 입력하세요"
            android:gravity="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.425"
            app:layout_constraintHorizontal_bias="0.501"/>
    <EditText
            android:id="@+id/semiMana_Edit_HospitalName"
            android:layout_width="307dp"
            android:layout_height="58dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:hint="병원이름을 입력하세요"
            android:gravity="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.335"
            app:layout_constraintHorizontal_bias="0.501"/>
    <TextView
            android:text="관리자용 회원가입"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/semi_mana_textView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="30dp"
            android:textSize="34sp"
            android:textStyle="bold"/>

    <Button
            android:id="@+id/semiMana_Btn_Finish"
            android:text="관리자 등록"
            android:layout_width="146dp"
            android:layout_height="80dp"
            android:background="@drawable/button_round"
            android:textSize="20dp"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="360dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>