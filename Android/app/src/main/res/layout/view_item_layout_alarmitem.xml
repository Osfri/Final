<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:id="@+id/constraintLayout"
                                                   android:layout_width="match_parent" android:layout_height="70dp">

    <TextView
            android:text="D"
            android:layout_width="57dp"
            android:layout_height="31dp" android:id="@+id/Alarm_name"
            android:textAlignment="center"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            android:layout_marginEnd="527dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="2020-10-23 23:23"
            android:layout_width="133dp"
            android:layout_height="31dp" android:id="@+id/Alarm_starttime"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="19dp" android:layout_marginStart="7dp"
            app:layout_constraintStart_toEndOf="@+id/Alarm_name" android:layout_marginEnd="387dp"
            app:layout_constraintEnd_toEndOf="parent" android:textAlignment="center"/>
    <TextView
            android:text="~"
            android:layout_width="20dp"
            android:layout_height="19dp" android:id="@+id/Alarm_text"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="28dp"
            app:layout_constraintStart_toEndOf="@+id/Alarm_starttime"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0" android:textAlignment="center"/>
    <TextView
            android:layout_width="133dp"
            android:layout_height="31dp" android:id="@+id/Alarm_endtime"
            android:textAlignment="center"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="20dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="19dp"
            app:layout_constraintStart_toEndOf="@+id/Alarm_text" android:layout_marginEnd="234dp"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>