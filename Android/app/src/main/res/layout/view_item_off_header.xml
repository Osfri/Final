<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content" android:background="#00FFFFFF">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/calendar_item_headText"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="18dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
    />
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints" android:id="@+id/linearLayout3">


        <LinearLayout
                android:id="@+id/ceffae"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_marginTop="?attr/actionBarSize"
                android:layout_marginBottom="10dp"
                android:layout_height="20dp" tools:ignore="MissingConstraints">

            <TextView
                    android:text="일"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView2"
                    android:textSize="12dp"
                    android:gravity="center"
                    android:layout_weight="1"/>
            <TextView
                    android:text="월"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView3"
                    android:textSize="12dp"
                    android:gravity="center"
                    android:layout_weight="1"/>
            <TextView
                    android:text="화"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView4"
                    android:textSize="12dp"
                    android:gravity="center"
                    android:layout_weight="1"/>
            <TextView
                    android:text="수"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    android:textSize="12dp"
                    android:gravity="center"
                    android:layout_weight="1"/>
            <TextView
                    android:text="목"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView11"
                    android:textSize="12dp"
                    android:gravity="center"
                    android:layout_weight="1"/>
            <TextView
                    android:text="금"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView12"
                    android:textSize="12dp"
                    android:gravity="center"
                    android:layout_weight="1"/>
            <TextView
                    android:text="토"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView13"
                    android:textSize="12dp"
                    android:gravity="center"
                    android:layout_weight="1"/>

        </LinearLayout>


    </LinearLayout>
    <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#A6A6A6"
            tools:ignore="MissingConstraints"
            app:layout_constraintBottom_toBottomOf="@+id/linearLayout3"/>


</androidx.constraintlayout.widget.ConstraintLayout>