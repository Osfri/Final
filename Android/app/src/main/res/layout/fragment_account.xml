<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".chat.fragment.AccountFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <ImageView
                android:src="@drawable/outline_account_circle_24"
                android:layout_width="200dp"
                android:layout_height="200dp" android:id="@+id/imageView3"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="120dp" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="사용자 휴대폰번호"
                android:layout_width="wrap_content"
                android:layout_height="30dp" android:id="@+id/accountfragment_userPhonenumber"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/accountfragment_userEmail"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toEndOf="@+id/textView9" android:gravity="center_vertical"/>
        <TextView
                android:text="사용자 아이디"
                android:layout_width="wrap_content"
                android:layout_height="30dp" android:id="@+id/accountfragment_userId"
                android:layout_marginTop="76dp"
                app:layout_constraintTop_toBottomOf="@+id/imageView3"
                app:layout_constraintStart_toEndOf="@+id/textView6" android:layout_marginStart="16dp"
                android:gravity="center_vertical"/>
        <TextView
                android:text="사용자 이메일"
                android:layout_width="wrap_content"
                android:layout_height="30dp" android:id="@+id/accountfragment_userEmail"
                app:layout_constraintTop_toBottomOf="@+id/accountfragment_userName"
                android:layout_marginTop="20dp"
                android:layout_marginStart="16dp" app:layout_constraintStart_toEndOf="@+id/textView8"
                android:gravity="center_vertical"/>
        <TextView
                android:text="아이디"
                android:layout_width="100dp"
                android:layout_height="30dp" android:id="@+id/textView6"
                android:layout_marginTop="76dp"

                app:layout_constraintTop_toBottomOf="@+id/imageView3"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="150dp" android:textStyle="bold" android:gravity="right"
                android:textSize="20sp" app:layout_constraintHorizontal_bias="0.542"/>
        <TextView
                android:text="이름"
                android:layout_width="100dp"
                android:layout_height="30dp" android:id="@+id/textView7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="150dp" android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/textView6" android:textStyle="bold"
                android:gravity="right"
                android:textSize="20sp" app:layout_constraintHorizontal_bias="0.542"/>
        <TextView
                android:text="이메일"
                android:layout_width="100dp"
                android:layout_height="30dp" android:id="@+id/textView8"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/textView7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="150dp"
                android:textStyle="bold"
                android:gravity="right" android:textSize="20sp" app:layout_constraintHorizontal_bias="0.542"/>
        <TextView
                android:text="휴대폰 번호"
                android:layout_width="100dp"
                android:layout_height="30dp" android:id="@+id/textView9"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/textView8" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="150dp" android:textStyle="bold"
                android:gravity="right" android:textSize="20sp" app:layout_constraintHorizontal_bias="0.542"/>
        <TextView
                android:text="소속"
                android:layout_width="100dp"
                android:layout_height="30dp" android:id="@+id/textView10"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="150dp" android:textStyle="bold"
                android:gravity="right" android:textSize="20sp" app:layout_constraintHorizontal_bias="0.542"/>
        <TextView
                android:text="사용자 이름"
                android:layout_width="wrap_content"
                android:layout_height="30dp" android:id="@+id/accountfragment_userName"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/accountfragment_userId"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toEndOf="@+id/textView7" android:gravity="center_vertical"/>
        <TextView
                android:text="사용자 소속"
                android:layout_width="wrap_content"
                android:layout_height="30dp" android:id="@+id/accountfragment_userCode"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/accountfragment_userPhonenumber"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toEndOf="@+id/textView10" android:gravity="center_vertical"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>