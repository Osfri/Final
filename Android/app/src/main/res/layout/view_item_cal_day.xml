<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="100dp"
                                                   android:orientation="vertical" android:background="#33FFFFFF"
>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cal_item_addContent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cal_item_dayText">
    </LinearLayout>
    <TextView
            android:id="@+id/cal_item_dayText"
            android:text="일"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="9dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
            android:layout_width="82dp"
            android:layout_height="71dp" tools:srcCompat="@tools:sample/avatars" android:id="@+id/cal_item_imageView"
            app:layout_constraintTop_toBottomOf="@+id/cal_item_dayText"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.875"/>

</androidx.constraintlayout.widget.ConstraintLayout>