<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="80dp">

    <TextView
            android:id="@+id/managerRecycleViewStaffName"
            android:text="맴버정보"
            android:gravity="left|center"
            android:layout_width="231dp"
            android:layout_height="36dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="28dp" android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.058"/>
    <Button
            android:id="@+id/manage_btn_staffdelete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="강퇴"
            android:background="#00ff0000"
            android:textColor="@color/pointred"
            android:textStyle="bold"
            android:textSize="14dp"
            app:layout_constraintStart_toEndOf="@+id/managerRecycleViewStaffName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="172dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#D6D6D6" tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="79dp" android:id="@+id/view" app:layout_constraintStart_toStartOf="parent"/>



</androidx.constraintlayout.widget.ConstraintLayout>